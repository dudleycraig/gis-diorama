{
  "name": "client",
  "description": "GIS and WebGL diorama frontend rendering",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "shadow:watch": "cross-env NODE_ENV=develop shadow-cljs watch app -A:client:common",
    "shadow:release": "shadow-cljs release app",
    "sass:watch": "sass --watch --update --style=expanded ./src/cljs/styles/core.scss:./public/css/core.css",
    "sass:dev": "concurrently 'npm:shadow:watch' 'npm:sass:watch'",
    "postcss:build": "cross-env TAILWIND_MODE=build postcss ./src/cljs/styles/core.scss -o ./public/css/core.css --verbose",
    "postcss:watch": "cross-env TAILWIND_MODE=watch postcss ./src/cljs/styles/core.scss -o ./public/css/core.css --verbose -w",
    "postcss:release": "cross-env NODE_ENV=production postcss ./src/cljs/styles/core.scss -o ./public/css/core.css --verbose",
    "start": "concurrently 'npm:shadow:watch' 'npm:postcss:watch'",
    "backend": "iced repl -A:dev:server:vim"
  },
  "browserslist": [
    "last 1 Chrome version"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.14.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-brands-svg-icons": "^5.15.2",
    "@fortawesome/free-regular-svg-icons": "^5.15.2",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@headlessui/react": "^1.6.6",
    "@react-three/drei": "^4.3.3",
    "@react-three/fiber": "^6.0.1",
    "@restart/hooks": "0.3.27",
    "@tailwindcss/line-clamp": "^0.4.2",
    "core-js": "^3.21.1",
    "create-react-class": "^15.6.2",
    "drei": "^2.2.21",
    "firebase": "^9.4.0",
    "prop-types-extra": "^1.1.1",
    "react": "^17.0.2",
    "react-colorful": "^4.4.4",
    "react-dom": "^17.0.2",
    "react-overlays": "^5.1.1",
    "react-refresh": "0.8.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "regenerator-runtime": "^0.13.9",
    "styled-components": "^5.3.6",
    "three": "^0.131.1",
    "three-stdlib": "^2.9.1"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.16.11",
    "autoprefixer": "^10.3.3",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-macros": "^3.1.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "firebase-admin": "^10.0.0",
    "firebase-functions": "^3.16.0",
    "firebase-tools": "^9.22.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "postcss-cli": "^9.0.2",
    "postcss-import": "^14.0.2",
    "postcss-nested": "^5.0.6",
    "postcss-scss": "^4.0.4",
    "sass": "^1.53.0",
    "sass-loader": "^10.2.1",
    "shadow-cljs": "^2.15.8",
    "stylelint": "^13.7.1",
    "tailwindcss": "^3.0.0",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.7.2"
  }
}
